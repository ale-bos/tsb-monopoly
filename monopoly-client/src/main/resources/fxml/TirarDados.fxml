                  </Label>
                  <HBox fx:id="hbPanel" alignment="CENTER" spacing="20.0">
                     <children>
                        <ImageView fitHeight="61.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/dados/dice.png" />
                           </image>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </ImageView>
                        <Button fx:id="btnTirarDados" defaultButton="true" mnemonicParsing="false" onAction="#processTirarDados" prefHeight="38.0" prefWidth="154.0" text="Tirar Dados" HBox.hgrow="ALWAYS" />
