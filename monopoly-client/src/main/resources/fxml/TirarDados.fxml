<StackPane maxHeight="226.0" maxWidth="539.0" minHeight="142.0" minWidth="326.0" prefHeight="156.0" prefWidth="326.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="monopoly.client.controller.TirarDadosController">
   <children>
      <AnchorPane>
         <children>
            <VBox fx:id="vbPanel" alignment="CENTER" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
